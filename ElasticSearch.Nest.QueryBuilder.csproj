<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>ElasticSearch.Nest.QueryBuilder</Title>
    <Description>To make Elastic Search Nest Query builder experience smooth</Description>
    <PackageTags>ElasticSearch, NEST, C#, Query Builder</PackageTags>
    <PackageId>ES.Nest.QueryBuilder</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NEST" Version="7.17.5" />
  </ItemGroup>

</Project>
